{
  "name": "campus",
  "version": "5.4.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-no-cache": "react-native start --reset-cache",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "full-check": "npm run typescript && npm run lint && npm run test",
    "pod": "cd ios && pod install && cd ..",
    "bundle": "cd android && ./gradlew bundleRelease",
    "clean": "react-native-clean-project",
    "postversion": "react-native-version",
    "test-locales": "node ./scripts/test-locales.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.16.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-community/slider": "4.1.12",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/stack": "6.0.11",
    "i18n-js": "3.8.0",
    "moment": "2.29.1",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-animatable": "1.3.3",
    "react-native-app-intro-slider": "4.0.4",
    "react-native-autolink": "4.0.0",
    "react-native-calendars": "1.1269.0",
    "react-native-collapsible": "1.6.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-zoom-viewer": "3.0.1",
    "react-native-keychain": "4.0.5",
    "react-native-linear-gradient": "2.5.6",
    "react-native-localize": "2.1.5",
    "react-native-modalize": "2.0.12",
    "react-native-paper": "4.10.1",
    "react-native-permissions": "3.1.0",
    "react-native-push-notification": "8.1.1",
    "react-native-reanimated": "2.14.1",
    "react-native-render-html": "6.3.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.13.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-timeago": "0.5.0",
    "react-native-vector-icons": "9.2.0",
    "react-native-webview": "11.17.2",
    "react-navigation-collapsible": "6.0.0",
    "react-navigation-header-buttons": "9.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.0",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "3.0.1",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.3",
    "@types/react-native": "0.66.5",
    "@types/react-native-calendars": "1.1264.3",
    "@types/react-native-push-notification": "7.3.3",
    "@types/react-native-vector-icons": "6.4.10",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "babel-jest": "26.6.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "jest-fetch-mock": "3.0.3",
    "metro-react-native-babel-preset": "0.72.3",
    "node-i18n-checker": "0.0.88322785",
    "prettier": "2.4.1",
    "react-native-clean-project": "3.6.7",
    "react-native-version": "4.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "4.5.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "no-undef": 0,
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "error"
      ],
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
